{
  "name": "reactProjectTemplate",
  "version": "0.1.0",
  "description": "Project template that sets up a fully functioning react.js application with webpack.",
  "main": "assets/js/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mvilrokx/react-starter-template.git"
  },
  "keywords": [
    "template",
    "react",
    "webpack",
    "hot module replacement",
    "ES6",
    "quickstart"
  ],
  "devDependencies": {
    "autoprefixer-core": "^5.2.0",
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.11",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.4",
    "csswring": "^3.0.5",
    "eslint": "^0.22.1",
    "eslint-friendly-formatter": "^1.0.8",
    "eslint-loader": "^0.12.0",
    "eslint-plugin-react": "^2.5.0",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.4.0",
    "image-webpack-loader": "^1.4.0",
    "postcss-loader": "^0.4.4",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.7",
    "react-in-style": "^0.21.0",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress --colors --config webpack.production.config.js",
    "openDev": "open http://localhost:8085/webpack-dev-server/ && npm run dev",
    "dev": "webpack-dev-server --progress --colors --hot --content-base dist --port 8085",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "start": "node server.js",
    "webcam-record": "ffmpeg -f avfoundation -pix_fmt uyvy422 -i \"0\" -vf scale=640:360 -segment_wrap 20 -f segment test-%d.webm"
  },
  "author": "Mark Vilrokx <mvilrokx@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvilrokx/react-starter-template/issues"
  },
  "homepage": "https://github.com/mvilrokx/react-starter-template"
}
